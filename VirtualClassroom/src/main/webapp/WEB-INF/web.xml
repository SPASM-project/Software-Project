<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ServletTeacher</servlet-name>
  	<display-name>ServletTeacher</display-name>
  	<description></description>
  	<servlet-class>com.SPASM.controller.ServletTeacher</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletStudent</servlet-name>
  	<display-name>ServletStudent</display-name>
  	<description></description>
  	<servlet-class>com.SPASM.controller.ServletStudent</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>PostServlet</servlet-name>
  	<display-name>PostServlet</display-name>
  	<description></description>
  	<servlet-class>com.SPASM.controller.PostServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ServletTeacher</servlet-name>
  	<url-pattern>/ServletTeacher</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletStudent</servlet-name>
  	<url-pattern>/ServletStudent</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>PostServlet</servlet-name>
  	<url-pattern>/PostServlet</url-pattern>
  </servlet-mapping>
</web-app>
